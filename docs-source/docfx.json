{
  "build": {
    "content": [
      {
        "files": [
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appFooter": "Copyright 2017-2023, Auth0 Inc.",
      "_appLogoPath": "images/logo.png",
      "_gitContribute": {
        "branch": "master"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "custom-template"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}